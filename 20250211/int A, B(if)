#include <stdio.h>

void main()
{
    int a = 0;
    int b = 0;
    
    scanf("%d", &a);
    scanf("%d", &b);
    
    if(a != 0 && b != 0)
    {
        if (a > b && a % b == 0)
        {
            printf("A와 B를 나눈 값은 %d", a / b);
        } else if (a > b && a % b != 0) {
            printf("A와 B를 나눈 나머지 값은 %d", a % b);
        } else if (a < b) {
            printf("A와 B를 더한 값은 %d", a + b);
        } else {
            printf("A: %d, B: %d", a, b);
        }
    } else {
        printf("Error");
    }
}
