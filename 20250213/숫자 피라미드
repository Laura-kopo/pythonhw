//C언어_2: 숫자 피라미드 만들기
#include <stdio.h>

void main()
{
    int n = 0;
    
    scanf("%d", &n);
    
    for (int i = 1; i <= n; i++) {
        for (int k = 1 + i; k <= n; k++) {
            printf(" ");
        }
        for (int j = 1; j <= i; j++) {
            printf("%d ", j);
        }
        printf("\n");
    }
}
