//2025년2월10일
//C언어 과제_1
#include <stdio.h>

void main()
{
    double result = 0;
    double a = 0;
    double b = 0;
    double c = 0;
    
    scanf("%lf", &a);
    scanf("%lf", &b);
    scanf("%lf", &c);
    
    if (c != 0)
    {
        result = (a + b) * c - (a - b) / c;
        printf("result = %lf\n", result);
    } else {
        printf("Exception");
    }
}

//C언어 과제_2
#include <stdio.h>

void main()
{
    int a = 0;
    int f = 0;
    int s = 0;
    int t = 0;
    
    scanf("%d", &a);
    
    f = a / 100;
    s = a / 10 - f * 10;
    t = a - f * 100 - s * 10;
    
    printf("%d, %d, %d", f, s, t);
}
